{
    "expo": {
        "name": "Twister",
        "version": "1.0.0",
        "slug": "twister",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "githubUrl": "https://github.com/rupson/react-native-twister",
        "splash": {
            "image": "./assets/images/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "rupson.react_native_twister.rupson.com.github"
        },
        "web": {
            "favicon": "./assets/images/favicon.png"
        }
    }
}
